<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Dynamic pointers -->
    <drawable name="album_art">@drawable/ic_album</drawable>
    <drawable name="artist_art">@drawable/ic_groups</drawable>
    <drawable name="genres_art">@drawable/ic_genres</drawable>
    <drawable name="dates_art">@drawable/ic_schedule</drawable>
    <drawable name="pause_anim">@drawable/ic_media_pause_anim</drawable>
    <drawable name="play_anim">@drawable/ic_media_play_anim</drawable>

    <!-- Media3 -->
    <drawable name="media3_notification_play" tools:keep="@drawable/media3_notification_play">@drawable/ic_play_arrow_filled</drawable>
    <drawable name="media3_notification_pause" tools:keep="@drawable/media3_notification_pause">@drawable/ic_pause_filled</drawable>
    <drawable name="media3_notification_seek_to_previous" tools:keep="@drawable/media3_notification_seek_to_previous">@drawable/ic_skip_previous_filled</drawable>
    <drawable name="media3_notification_seek_to_next" tools:keep="@drawable/media3_notification_seek_to_next">@drawable/ic_skip_next_filled</drawable>
</resources>